{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@now/node-server"
    },
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
          "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "server/index.ts"
    }
  ]
}
